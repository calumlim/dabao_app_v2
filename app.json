{
  "expo": {
    "name": "dabao-app",
    "slug": "dabao-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.calum.dabao"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bao_logo.png",
    "splash": {
      "image": "./assets/images/bao_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calum.dabao"
    }
  }
}
